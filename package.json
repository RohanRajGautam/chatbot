{
  "name": "chatbot",
  "version": "1.0.0",
  "description": "Chatbot for webpage create using React in the front-end, Node in the backend and MongoDB for the database.",
  "main": "index.js",
  "engines": {
    "node": "12.18.0",
    "npm": "6.14.8"
  },
  "scripts": {
    "start": "node index.js",
    "backend": "nodemon index.js",
    "frontend": "npm run start --prefix client",
    "dev": "concurrently \"npm run backend\" \"npm run frontend\""
  },
  "author": "Rohan Raj Gautam",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.19.0",
    "dialogflow": "^1.2.0",
    "express": "^4.17.1",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.6"
  }
}
